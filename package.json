{
  "name": "attendance-management",
  "productName": "attendance-management",
  "version": "0.9.12",
  "author": "Taichi Murakami",
  "private": true,
  "main": "public/electron/main.js",
  "homepage": "./",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/tests/**/*.test.ts",
      "**/src/tests/*.test.ts"
    ]
  },
  "scripts": {
    "react-start": "cross-env BROWSER=none react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-start": "wait-on http://localhost:3000 && electron .",
    "electron-build": "electron-builder",
    "start": "run-p react-start electron-start",
    "build": "run-s react-build electron-build",
    "test": "jest"
  },
  "build": {
    "appId": "com.electron.attendance-management_v1.00.app",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extends": null,
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts-react-default": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.0",
    "file-type": "^17.1.1",
    "sass": "^1.51.0",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^28.0.3",
    "mini-css-extract-plugin": "^2.2.2",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "sass-loader": "^12.1.0",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "wait-on": "^6.0.0",
    "webpack": "^5.72.0"
  }
}
